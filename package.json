{
  "name": "wix-discount-competition-app",
  "version": "1.0.0",
  "description": "AI-powered discount competition plugin for Wix stores",
  "main": "src/backend/server.js",
  "scripts": {
    "start": "node src/backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/backend/server.js",
    "dev:frontend": "react-scripts start",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "react-scripts build",
    "build:backend": "babel src/backend -d dist/backend",
    "build:production": "NODE_ENV=production npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "react-scripts test --watchAll=false",
    "test:backend": "jest --config jest.backend.config.js",
    "test:e2e": "playwright test",
    "test:watch": "concurrently \"npm run test:frontend -- --watch\" \"npm run test:backend -- --watch\"",
    "test:coverage": "npm run test:frontend -- --coverage && npm run test:backend -- --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "deploy:staging": "wix deploy --environment staging",
    "deploy:production": "wix deploy --environment production",
    "wix:init": "wix create app",
    "wix:dev": "wix dev",
    "wix:build": "wix build",
    "wix:deploy": "wix deploy"
  },
  "keywords": [
    "wix",
    "plugin",
    "discount",
    "competition",
    "ecommerce",
    "ai",
    "gamification",
    "marketing"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@wix/design-system": "^9.0.0",
    "@wix/sdk": "^1.5.0",
    "@wix/api-client": "^1.0.0",
    "@wix/stores": "^1.0.0",
    "@wix/marketing": "^1.0.0",
    "@wix/members": "^1.0.0",
    "@wix/business-manager-api": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-query": "^3.39.0",
    "@reduxjs/toolkit": "^1.9.0",
    "react-redux": "^8.0.5",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^6.0.0",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "mongoose": "^6.10.0",
    "redis": "^4.6.0",
    "axios": "^1.3.0",
    "openai": "^4.0.0",
    "anthropic-sdk": "^0.10.0",
    "joi": "^17.8.0",
    "winston": "^3.8.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "multer": "^1.4.5",
    "sharp": "^0.32.0",
    "nodemailer": "^6.9.0",
    "cron": "^2.2.0",
    "socket.io": "^4.6.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.0",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.0",
    "@babel/preset-typescript": "^7.18.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.0",
    "jest": "^29.4.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/user-event": "^14.4.0",
    "playwright": "^1.31.0",
    "@playwright/test": "^1.31.0",
    "supertest": "^6.3.0",
    "nock": "^13.3.0",
    "eslint": "^8.35.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.0",
    "nodemon": "^2.0.0",
    "concurrently": "^7.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/setupTests.js"
    ]
  },
  "proxy": "http://localhost:3001",
  "homepage": "."
}